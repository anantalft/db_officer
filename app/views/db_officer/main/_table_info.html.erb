<%= flash[:message]%>
<h3>Table: <%=params[:table_name]%>
  <%= link_to_edit(params[:table_name])%>
  <%= link_to_delete(params[:table_name]) %></h3>
<table class="flat-table">
  <tr><th>#</th><th>Name</th><th>Type</th><th colspan="2">Action</th></tr>
  <% count = 0%>
  <%@columns.each do |column|%>
    <tr><td><%= count = count + 1%></td><td><%= column.name %></td><td><%=
        column.type.to_s
    %></td><td colspan="2">
      <%= link_to_edit_column(params[:table_name],column.name)%>
      <%= link_to_delete_column %></td></tr>
  <% end%>
</table>

